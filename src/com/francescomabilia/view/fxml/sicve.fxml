<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          alignment="CENTER"
          fx:controller="com.francescomabilia.controller.Sicve"
          prefHeight="400.0" prefWidth="600.0">

    <Label text="SICVE" GridPane.rowIndex="0" GridPane.columnIndex="0" style="-fx-font-size: 20; -fx-font-family: 'Times New Roman'"/>

    <Button fx:id="buttonAutoveicolo" text="Autoveicolo" GridPane.rowIndex="1" GridPane.columnIndex="0"  prefHeight="42" prefWidth="136" />

    <Label text="" GridPane.rowIndex="2" GridPane.columnIndex="0"/>

    <Button fx:id="buttonAmministratore" text="Amministratore" GridPane.rowIndex="3" GridPane.columnIndex="0"  prefHeight="42" prefWidth="136" />

    <Label text="" GridPane.rowIndex="4" GridPane.columnIndex="0"/>

    <Button fx:id="buttonPolizia" text="Polizia" GridPane.rowIndex="5" GridPane.columnIndex="0" prefHeight="42" prefWidth="136"/>
</GridPane>